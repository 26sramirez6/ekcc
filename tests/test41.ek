def void inc (ref int $n) {
    $n = $n + 1;
}

def void dec (ref int $n) {
    $n = $n - 1;
}

def int power(int $n, int $v){
    int $i = 0;
    int $r = 1;
    while($i < $n){
        $r = $r * $v;
        inc($i);
    }
    return $r;
}

def int reverseInt(int $n){
    int $x = $n;
    int $r = 0;
    int $i = 0;
    int $j = 0;
    while($x/10 > 1){
        $x = $x/10;
        inc($i);
    }

    while($i > -1){
        $r = $r + $n/power($i, 10) * power($j, 10);
        $n = $n - $n/power($i, 10) * power($i, 10);
        dec($i);
        inc($j);
    }
    return $r;
}

def int run(){
    int $x = reverseInt(52165476);
    return $x;
}